<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5A875C"
    tools:context=".UdattaForestActivity">

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="116dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/udawathta_forest" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="373dp"
        android:layout_height="101dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/white_half_background"
        tools:layout_editor_absoluteY="38dp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="303dp"
        android:layout_height="41dp"
        android:layout_marginBottom="32dp"
        android:text="Udawatta Kele Sanctuary"
        android:textColor="@color/black"
        android:textSize="27dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="@+id/imageView32"
        app:layout_constraintEnd_toEndOf="@+id/imageView32"
        app:layout_constraintStart_toStartOf="@+id/imageView32" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="435dp"
        android:layout_height="108dp"
        android:layout_marginBottom="184dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/white_half_background" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="450dp"
        android:layout_height="88dp"
        android:layout_marginBottom="68dp"
        android:text="Udawattakele Forest Reserve often spelled as Udawatta Kele, \n is a historic forest reserve on a hill-ridge in the city of Kandy. \n It is 104 hectares large."
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView31"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView33"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="42dp"
        android:layout_height="43dp"
        android:layout_marginBottom="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/bed" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="36dp"
        android:layout_height="41dp"
        android:layout_marginBottom="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toEndOf="@+id/imageView34"
        app:srcCompat="@drawable/marker2" />
</androidx.constraintlayout.widget.ConstraintLayout>